{
	"name": "omnicode/lara-make",
	"description": "Advanced Make commands",
	"keywords": ["laravel", "bootstrapping", "make"],
	"license": "MIT",
	"authors": [
		{
			"name": "Omnicode"
		}
	],
	"require": {
		"laravel/framework": "5.5.*",
		"omnicode/lara-service": "~2.0",
		"omnicode/lara-support": "~2.0",
		"omnicode/lara-crud": "~2.0",
		"omnicode/lara-repo": "~2.0",
		"omnicode/lara-model": "~2.0",
		"omnicode/lara-validation": "~2.0",
		"omnicode/lara-view-composer": "~2.0"
	},
	"require-dev": {
		"mockery/mockery": "~1.0",
		"phpunit/phpunit": "~6.0"
	},
	"suggest": {
		"omnicode/lara-view-composer": "Useful view composers to populate dropdown lists"
	},
	"autoload": {
		"psr-4": {
			"LaraMake\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	}
}
