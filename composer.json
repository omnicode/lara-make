{
	"name": "omnicode/lara-make",
	"description": "Advanced Make commands",
	"keywords": ["laravel", "bootstrapping", "make"],
	"license": "MIT",
	"authors": [
		{
			"name": "Omnicode"
		}
	],
	"require": {
		"laravel/framework": "5.3.* || 5.4.* || 5.5.*",
		"omnicode/lara-service": "~0.0",
		"omnicode/lara-support": "~0.0",
		"omnicode/lara-crud": "~0.0",
		"omnicode/lara-repo": "~0.1",
		"omnicode/lara-model": "~0.0",
		"omnicode/lara-validation": "~0.0",
		"omnicode/lara-view-composer": "~0.0"
	},
	"require-dev": {
		"mockery/mockery": "~0.9",
		"phpunit/phpunit": "~4.0|~5.0"
	},
	"suggest": {
		"omnicode/lara-view-composer": "Useful view composers to populate dropdown lists"
	},
	"autoload": {
		"psr-4": {
			"LaraMake\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	}
}
