{
    "name": "omnicode/lara-make",
    "description": "Additions for laravel support",
    "keywords": ["laravel", "support"],
    "license": "MIT",
    "authors": [
        {
            "name": "Omnicode"
        }
    ],
    "require": {
        "laravel/framework": "5.6.*",
        "omnicode/lara-support": "~3.1",
        "omnicode/php-util": "0.0.*"
    },
    "require-dev": {
        "omnicode/lara-test": "~3.1",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"

    },
    "autoload": {
        "psr-4": {
            "LaraMake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraMake\\ServiceProvider\\LaraMakeServiceProvider"
            ]
        }
    }
}
